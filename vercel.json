{
  "crons": [
    {
      "path": "/api/steps",
      "schedule": "15 1 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/privacy",
      "destination": "/"
    },
    {
      "source": "/login",
      "destination": "/"
    },
    {
      "source": "/((?!api(?:/|$)).*)",
      "destination": "/"
    }
  ]
}
